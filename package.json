{
  "name": "iam",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "pylon build && wrangler deploy",
    "dev": "pylon dev -c \"wrangler dev --port 3000\" --client --client-port 3000 --client-path ../../../packages/gatsby-plugin-jaen/client/iam/index.ts",
    "cf-typegen": "wrangler types",
    "db:prepare": "./scripts/migrate.sh prepare",
    "db:deploy": "./scripts/migrate.sh deploy",
    "db:reset": "./scripts/migrate.sh reset",
    "db:status": "./scripts/migrate.sh status",
    "db:create": "./scripts/migrate.sh create",
    "db:studio": "./scripts/migrate.sh studio",
    "db:info": "./scripts/migrate.sh d1-info",
    "db:backup": "./scripts/migrate.sh d1-backup",
    "migrate": "pnpm db:prepare",
    "migrate:deploy": "pnpm db:deploy",
    "migrate:reset": "pnpm db:reset",
    "migrate:status": "pnpm db:status",
    "migrate:studio": "pnpm db:studio"
  },
  "dependencies": {
    "@envelop/core": "^5.4.0",
    "@getcronit/pylon": "^2.9.6",
    "@gqty/cli": "^5.0.0",
    "@prisma/adapter-d1": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@pushforge/builder": "^1.1.2",
    "gqty": "^3.6.0",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.10",
    "@cloudflare/workers-types": "^4.20251121.0",
    "@getcronit/pylon-dev": "^1.0.6",
    "prisma": "^7.0.0",
    "typescript": "^5.9.3",
    "vitest": "~3.2.4",
    "wrangler": "^4.50.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/getcronit/pylon.git"
  },
  "homepage": "https://pylon.cronit.io"
}
